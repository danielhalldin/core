{
  "name": "core",
  "version": "1.0.0",
  "description": "",
  "main": "/build/web.js",
  "scripts": {
    "dev": "nodemon --exec babel-node src/web.js -e js,graphql,json --node-path './src/web.js'",
    "dev-worker": "nodemon --watch src/worker.js --exec babel-node src/worker.js -e js,graphql,json --node-path './src/worker.js'",
    "prepush": "yarn lint",
    "build": "babel src --copy-files --source-maps --out-dir build",
    "start": "node build src/web.js && node build src/worker.js",
    "test": "jest src",
    "prestart": "yarn build",
    "lint": "eslint src",
    "security": "nsp check",
    "killProcess": "yarn run build && kill $(lsof -t -i:4444) || true",
    "postinstall": "babel src --copy-files --source-maps --out-dir build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-cache-control": "^0.2.3",
    "apollo-datasource-rest": "^0.1.5",
    "apollo-link-http": "^1.5.4",
    "apollo-server": "^2.1.0",
    "apollo-server-cache-redis": "^0.1.3",
    "apollo-server-express": "^2.0.6",
    "babel-eslint": "^9.0.0",
    "compression": "^1.7.3",
    "cookies": "^0.7.2",
    "elasticsearch": "^15.1.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "express": "^4.16.3",
    "graphql": "^14.0.2",
    "jwt-simple": "^0.5.5",
    "moment": "^2.22.2",
    "morgan": "^1.9.1",
    "node-fetch": "^2.2.0",
    "querystring": "^0.2.0",
    "redis": "^2.8.0",
    "throng": "^4.0.0",
    "urijs": "^1.19.1",
    "winston": "^3.1.0",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.18.0",
    "husky": "^0.14.3",
    "jest": "^23.4.2",
    "nodemon": "^1.18.3",
    "rimraf": "^2.6.2",
    "standard": "^11.0.1"
  }
}
